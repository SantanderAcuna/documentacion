{
  "name": "cms-frontend-admin",
  "version": "1.0.0",
  "type": "module",
  "description": "Panel administrativo CMS Gubernamental",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
